            {% assign type = itemPreset[1] %}
          {% endif %}
          {% if itemPreset[0] == "base" %}
            {% assign type = itemPreset[1] %}
          {% endif %}
        {%- endfor -%}
        {% assign optionRendered = true %}
        {%- for value in values -%}
          
    {%- liquid
      assign option_disabled = true

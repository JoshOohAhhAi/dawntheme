      style="--gtc:repeat(1, minmax(0, 1fr));--ta:left;--w:100%;--w-tablet:100%;--w-mobile:100%;--rg:0px;--rg-mobile:var(--g-s-xl)"
    >
      
      {% assign presets = "base($2)dropdown($1)SELECT YOUR COLOR($2)color($1)Size($2)dropdown($1)Color($2)color($1)Size 2($2)rectangle_list($1)Select Color($2)image_shopify($1)SELECT YOUR SIZE($2)dropdown($1)SELECT COLOR($2)color($1)SELECT SIZE($2)rectangle_list($1)Select Size($2)rectangle_list($1)GemSleep Pack($2)rectangle_list($1)Option($2)rectangle_list($1)Camera Style($2)rectangle_list($1)Style($2)dropdown($1)Serving Size($2)rectangle_list($1)Color Option($2)dropdown($1)Colors($2)color($1)COLOR($2)rectangle_list($1)Size 3($2)dropdown($1)Storage($2)rectangle_list" | split: '($1)' %}
